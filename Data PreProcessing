    "#categorize car class by price\n",
    "bins = [0, 10000, 20000, 30000, float('inf')]\n",
    "labels = ['Low Class', 'Mid Class', 'High Class', 'Premium Class']\n",
    "df['class_by_price'] = pd.cut(df['price'], bins=bins, labels=labels, include_lowest=True)\n",
    "\n",
    "df"
   ]
